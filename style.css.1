/*accordion classes*/

#nestedAccordion{
	width:600px;
}
#nestedAccordion li, #module_accordion li{
	margin:0;
	font-size:1em;
	font-weight:normal;
	list-style:none;
	padding: 0 0 0 30px;
}
#nestedAccordion h2, #nestedAccordion h3, 
#nestedAccordion div, #nestedAccordion ol, {
	/* reset styles for accordion */
	margin:0;
	font-size:1em;
	font-weight:normal;
	list-style:none;
}
#nestedAccordion div{
	display:none;
	color:#555;
	overflow:hidden;
}
#nestedAccordion h2{
	cursor:pointer;
	color:#000;
	moz-border-radius: 5px 5px 0 0;
	-webkit-border-radius: 5px 5px 0 0;
	border-radius: 5px 5px 0 0;
	margin-top:5px;
	padding-left:7px;
	font-size:18px;
	font-family:Arial;
	font-weight:normal;
	background-color: #cee0f8;
	background-image: -webkit-gradient(linear, left top, left bottom, from(#cee0f8), to(#e1e4ef));
	background-image: -webkit-linear-gradient(top, #cee0f8, #e1e4ef);
	background-image: -moz-linear-gradient(top, #cee0f8, #e1e4ef);
	background-image: -ms-linear-gradient(top, #cee0f8, #e1e4ef);
	background-image: -o-linear-gradient(top, #cee0f8, #e1e4ef);
	background-image: linear-gradient(to bottom, #cee0f8, #e1e4ef);
}
.accordionImg img{
	vertical-align:middle;
	padding: 5px;
	margin-right:7px;
}
#nestedAccordion h3, #nestedAccordionModule h3{
	cursor:pointer;
	padding:5px;
	color: #666;
	font-weight:bold;
	text-shadow: 0 1px 0 rgba(255, 255, 255, .5);
	font-size:15px;
	font-family:Arial;
	font-weight:normal;
	background-color: #f5f5f5;
	background-image: -webkit-gradient(linear, left top, left bottom, from(#f5f5f5), to(#ededed));
	background-image: -webkit-linear-gradient(top, #f5f5f5, #ededed);
	background-image: -moz-linear-gradient(top, #f5f5f5, #ededed);
	background-image: -ms-linear-gradient(top, #f5f5f5, #ededed);
	background-image: -o-linear-gradient(top, #f5f5f5, #ededed);
	background-image: linear-gradient(to bottom, #f5f5f5, #ededed);

}
#nestedAccordion h3:hover{
	color:#000;
}
#nestedAccordion h3 + div{
	background-color:#FFFFFF;
	padding:5px 5px 5px 30px;
}
#nestedAccordion li:hover{
	color:#000;
}
#nestedAccordionModule{
width: 90%;
text-align:left;
}
#nestedAccordionModule h2{
	cursor:pointer;
	color:#000;
	moz-border-radius: 5px 5px 0 0;
	-webkit-border-radius: 5px 5px 0 0;
	border-radius: 5px 5px 0 0;
	margin-top:5px;
	padding-left:30px;
	font-size:16px;
	font-family:Arial;
	font-weight:normal;
	background-color: #f5f5f5;
	background-image: -webkit-gradient(linear, left top, left bottom, from(#f5f5f5), to(#e1e4ef));
	background-image: -webkit-linear-gradient(top, #f5f5f5, #e1e4ef);
	background-image: -moz-linear-gradient(top, #f5f5f5, #e1e4ef);
	background-image: -ms-linear-gradient(top, #f5f5f5, #e1e4ef);
	background-image: -o-linear-gradient(top, #f5f5f5, #e1e4ef);
