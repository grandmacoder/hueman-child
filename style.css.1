/* blue rounded table */
.rounded_table {
	margin:0px;padding:0px;
	width:860px;
	box-shadow: 5px 5px 5px #888888;
	border:2px solid #ffffff;
	
	-moz-border-radius-bottomleft:3px;
	-webkit-border-bottom-left-radius:3px;
	border-bottom-left-radius:3px;
	
	-moz-border-radius-bottomright:3px;
	-webkit-border-bottom-right-radius:3px;
	border-bottom-right-radius:3px;
	
	-moz-border-radius-topright:3px;
	-webkit-border-top-right-radius:3px;
	border-top-right-radius:3px;
	
	-moz-border-radius-topleft:3px;
	-webkit-border-top-left-radius:3px;
	border-top-left-radius:3px;
}.rounded_table table{
    border-collapse: collapse;
    border-spacing: 0;
	width:100%;
	height:100%;
	margin:0px;padding:0px;
}.rounded_table tr:last-child td:last-child {
	-moz-border-radius-bottomright:3px;
	-webkit-border-bottom-right-radius:3px;
	border-bottom-right-radius:3px;
}
.rounded_table table tr:first-child td:first-child {
	-moz-border-radius-topleft:3px;
	-webkit-border-top-left-radius:3px;
	border-top-left-radius:3px;
}
.rounded_table table tr:first-child td:last-child {
	-moz-border-radius-topright:3px;
	-webkit-border-top-right-radius:3px;
	border-top-right-radius:3px;
}.rounded_table tr:last-child td:first-child{
	-moz-border-radius-bottomleft:3px;
	-webkit-border-bottom-left-radius:3px;
	border-bottom-left-radius:3px;
}.rounded_table tr:hover td{
	background-color:#d1e3ed;
}
.rounded_table td{
	vertical-align:middle;
	background-color:#f0f0f0;
    border:1px solid #ffffff;
	border-width:0px 1px 1px 0px;
	text-align:left;
	padding:7px;
	font-size:12px;
	font-family:Arial;
	font-weight:normal;
	color:#000000;
}.rounded_table tr:last-child td{
	border-width:0px 1px 0px 0px;
}.rounded_table tr td:last-child{
	border-width:0px 0px 1px 0px;
}.rounded_table tr:last-child td:last-child{
	border-width:0px 0px 0px 0px;
}
.rounded_table tr:first-child td{
	background:-o-linear-gradient(bottom, #3b8dbd 5%, #054d77 100%);	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #3b8dbd), color-stop(1, #054d77) );
	background:-moz-linear-gradient( center top, #3b8dbd 5%, #054d77 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr="#3b8dbd", endColorstr="#054d77");	background: -o-linear-gradient(top,#3b8dbd,003f7f);
    background-color:#3b8dbd;
	border:0px solid #ffffff;
	text-align:center;
	border-width:0px 0px 1px 1px;
	font-size:14px;
	font-family:Arial;
	font-weight:bold;
	color:#ffffff;
}

.rounded_table tr:first-child:hover td{
	background:-o-linear-gradient(bottom, #3b8dbd 5%, #054d77 100%);	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #003366), color-stop(1, #054d77) );
	background:-moz-linear-gradient( center top, #3b8dbd 5%, #054d77 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr="#3b8dbd", endColorstr="#003f7f");	background: -o-linear-gradient(top,#003366,054d77);
   background-color:#3b8dbd;
}
.rounded_table tr:first-child td:first-child{
	border-width:0px 0px 1px 0px;
}
.rounded_table tr:first-child td:last-child{
	border-width:0px 0px 1px 1px;
}

